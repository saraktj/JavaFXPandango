<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pandango.SearchResultsController">
   <children>
      <ImageView fx:id="image" fitHeight="132.0" fitWidth="142.0" layoutX="91.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true" />
      <Text fx:id="title" layoutX="207.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Button fx:id="back" layoutX="325.0" layoutY="340.0" mnemonicParsing="false" onAction="#btnClick" text="Back to Home Screen" />
      <Button fx:id="submit" layoutX="186.0" layoutY="340.0" mnemonicParsing="false" onAction="#btnClick" text="Submit Rating" />
      <HBox fx:id="ratingBox" layoutX="186.0" layoutY="248.0" prefHeight="73.0" prefWidth="200.0" />
      <TextArea fx:id="synopsis" editable="false" layoutX="207.0" layoutY="69.0" prefHeight="172.0" prefWidth="351.0" text="Synopsis: " />
      <TextArea fx:id="comments" layoutX="390.0" layoutY="248.0" prefHeight="82.0" prefWidth="168.0" promptText="comments" text="Comments:" />
   </children>
</AnchorPane>
